<div id="admin-add-seed-page">
<%= form_for :post, url: admin_actions_create_seed_path do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>
  
  <%= f.label :name %>
  <%= f.text_field :name %>
  
  <%= f.label :age %>
  <%= f.number_field :age %>
  
  <%= f.label :former_job %>
  <%= f.text_field :former_job %>
  <div>
    <%= f.submit "Add Content", class: "btn btn-primary" %>
  </div>
<% end %>
</div>